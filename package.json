{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "git": "run-s git:add git:commit git:push",
    "git:commit": "git commit -m \"update dependencies\"",
    "git:add": "git add package.json package-lock.json",
    "git:push": "git push",
    "dependencies:update": "ncu -u -x typescript,ngx-mqtt",
    "dependencies:install": "npm install",
    "update:dependencies and build": "run-s dependencies:update dependencies:install build git"
  },
  "dependencies": {
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/platform-express": "7.6.15",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.6",
    "ifttt-webhooks": "1.0.5",
    "pushover-notifications": "1.2.2",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "adblockbanner": "1.1.0",
    "@nestjs/cli": "7.5.6",
    "@nestjs/schematics": "7.3.0",
    "@nestjs/testing": "7.6.15",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.36",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "tsconfig-paths": "3.9.0",
    "npm-check-updates": "11.3.0",
    "typescript": "4.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
